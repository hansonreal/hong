<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.hong.log.mapper.LogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.github.hong.entity.sys.Log">
        <id column="LOG_ID" property="logId" />
        <result column="LOG_YM" property="logYm" />
        <result column="USER_ID" property="userId" />
        <result column="USER_NAME" property="userName" />
        <result column="USER_IP" property="userIp" />
        <result column="BIZ_MODE" property="bizMode" />
        <result column="METHOD_NAME" property="methodName" />
        <result column="METHOD_DESC" property="methodDesc" />
        <result column="REQU_URL" property="requUrl" />
        <result column="REQU_INFO" property="requInfo" />
        <result column="RESP_INFO" property="respInfo" />
        <result column="CREATE_TIME" property="createTime" />
        <result column="DURATION_TIME" property="durationTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        LOG_ID, LOG_YM, USER_ID, USER_NAME, USER_IP, BIZ_MODE, METHOD_NAME, METHOD_DESC, REQU_URL, REQU_INFO, RESP_INFO, CREATE_TIME, DURATION_TIME
    </sql>

</mapper>
